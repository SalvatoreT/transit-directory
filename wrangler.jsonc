{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "transit-directory",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-12-20",
  "preview_urls": true,
  "compatibility_flags": ["nodejs_compat"],
  "triggers": {
    "crons": ["0 * * * *"]
  },
  "build": {
    "command": "yarn build",
    "cwd": ".",
    "watch_dir": "./src"
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "gtfs_data",
      "database_name": "gtfs-data",
      "database_id": "69a1602e-4255-40e4-bd44-883503a04d44"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "SESSION",
      "id": "96d1374577344c278ecf8978ca4a0ab4"
    }
  ],
  "workflows": [
    {
      "name": "import-511-workflow",
      "binding": "IMPORT_511_WORKFLOW",
      "class_name": "Import511Workflow"
    }
  ],
  "r2_buckets": [
    {
      "bucket_name": "gtfs-processing",
      "binding": "gtfs_processing"
    }
  ]
}
